@model List<ClassLibrary1.Reportes>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewBag.Title = "ListR";
}

<div class="bg-dark text-light mt-4">
    <h2>Lista de reportes abiertos</h2>

    <hr />

    <table class="table text-light">
        <thead>
            <tr>
                <th>ID</th>
                <th>Equipo</th>
                
            </tr>
        </thead>


        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.IdReporte
                </td>
                <td>
                    @item.Equipo
                </td>
                <td>
                    <div class="btn btn-primary">
                        <a class="text-dark link1"> @Html.ActionLink("Detalles", "LastDetails", new { IdReporte = item.IdReporte })</a>
                    </div>
                </td>

            </tr>
        }

    </table>
    <a class="btn btn-warning text-dark" asp-action="Index">Volver a la pagina principal</a>
</div>